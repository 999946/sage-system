<!DOCTYPE html>
<html>
<!--
#set(List<BlogPreview> blogs)
#set(paginationLinks)
-->
<head>
  <meta charset="UTF-8">
  <title>博客列表</title>
  $!{allcss}
  <style>
    .btn-write-blog {margin: 10px}
  </style>
</head>
<body>
$!{include("navbar.httl")}
<div class="container" id="container">
  <div class="blog-list">
    <a class="btn-write-blog btn btn-default btn-sm" href="/blogs/new">写博客!</a>
    #for(blog : blogs)
      $!{include("blog-preview.httl")}
    #end
  </div>
  <div class="pagination">
    $!{paginationLinks}
  </div>
</div>
$!{alljs}
<script>
  setup(humanTime_setup, tag_setup, user_setup)
  $(document).ready(function() {
    $('.summary').warnEmpty().each(function(){
      $(this).html(marked($(this).text()))
    })
  })
</script>
</body>
</html>