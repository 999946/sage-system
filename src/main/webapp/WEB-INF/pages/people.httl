<!DOCTYPE html>
<html>
#set(components = ["bootstrap", "layout", "tag", "user"])
<head>
  <meta charset="UTF-8">
  <title>人们</title>
  $!{includeCSS()}
  <style>
    #container > div{
      clear: both;
    }
  </style>
</head>
<body>
$!{include("navbar.httl")}
<div id="container" class="container">
  <div><strong>推荐给你:</strong></div>
  <div id="recomms"></div>
  <div><strong>全部用户:</strong></div>
  <div id="people"></div>
</div>
$!{includeProto}
$!{includeJS()}
<script>
  'use strict'
  $(document).ready(function(){
    $.each(window.frontMap.recomms, function(i_, uc){
      createUserCard(uc).css('margin', '10px').css('float', 'left')
          .appendTo($('#recomms'))
    })
    $.each(window.frontMap.people, function(i_, uc){
      createUserCard(uc).css('margin', '10px').css('float', 'left')
          .appendTo($('#people'))
    })
  })
</script>
</body>
</html>