#set(FollowList fs)
#set(Boolean isSelf)
<div class="follow-list" data-id="${fs.id}">
  <div>
    <span>${fs.name}</span>
    #if(!isSelf)<a class="heed_follow_list" href="javascript:;">订阅</a>#end
  </div>
  #for(FollowInfo f : fs.list)
  <div class="finfo">
    $!{include('user-label.httl', ['u': f.user])}
    <div class="finfo-tags">
      #for(each : f.tags)
      $!{include('tag-label.httl')}
      #end
    </div>
  </div>
  #end
</div>